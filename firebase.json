{
  "hosting": {
    "site": "auto-notion",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/auth",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/signin",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/dashboard",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/dashboard/**",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/automation",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/ig-connections",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/content",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/super-agents",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/agents",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/cosmic",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/integrations",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/logs",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/settings",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/subscription",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/privacy",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/terms",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/refund-policy",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/data-deletion",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}