{
    "hosting": {
        "site": "auto-notion",
        "public": "dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/dashboard/**",
                "destination": "/dashboard/index.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}