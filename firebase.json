{
  "hosting": {
    "site": "auto-notion",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/auth",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/signin",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "/dashboard/**",
        "destination": "/dashboard/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}